  <div class="preloader-wrapper">
    <div class="preloader">
    </div>
  </div>

  <div class="offcanvas offcanvas-end" data-bs-scroll="true" tabindex="-1" id="offcanvasSearch"
    aria-labelledby="Search">
    <div class="offcanvas-header justify-content-center">
      <button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="Close"></button>
    </div>
    <div class="offcanvas-body">
      <div class="order-md-last">
        <h4 class="d-flex justify-content-between align-items-center mb-3">
          <span class="text-primary">Search</span>
        </h4>
        <form role="search" action="/search" method="GET" class="d-flex mt-3 gap-0">
          <input class="form-control rounded-start rounded-0 bg-light" type="email"
            placeholder="What are you looking for?" aria-label="What are you looking for?">
          <button class="btn btn-dark rounded-end rounded-0" type="submit">Search</button>
        </form>
      </div>
    </div>
  </div>

  <header>
    <div class="container-fluid" style="background-color: #bcd2d2;">
      <div class="row py-3 border-bottom">

        <!-- Logo Section -->
        <div class="col-sm-4 col-lg-3 text-center text-sm-start">
          <div class="main-logo">
            <a href="index.html">
              <img src="images/logo.png" alt="logo" class="img-fluid" style="max-width: 40%;">
            </a>
          </div>
        </div>

        <!-- Search Bar -->
        <div class="col-sm-6 offset-sm-2 offset-md-0 col-lg-5 d-none d-lg-block">
          <div class="search-bar row bg-light p-2 my-2 rounded-4">
            <div class="col-1">
              <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                <path fill="currentColor"
                  d="M21.71 20.29L18 16.61A9 9 0 1 0 16.61 18l3.68 3.68a1 1 0 0 0 1.42 0a1 1 0 0 0 0-1.39ZM11 18a7 7 0 1 1 7-7a7 7 0 0 1-7 7Z" />
              </svg>
            </div>
            <div class="col-11 col-md-7">
              <form id="search-form" class="text-center" action="/search" method="get">
                <input type="text" class="form-control border-0 bg-transparent"
                  placeholder="Search for more than 20,000 products" />
              </form>
            </div>
          </div>
        </div>
        <!-- Redesigned Icons Section (Sign Up, Login, Wishlist, Cart) -->
        <div
          class="col-sm-8 col-lg-4 d-flex justify-content-end gap-2 align-items-center mt-4 mt-sm-0 justify-content-center justify-content-sm-end">

          <!-- Support Box (Optional) -->
          <div class="support-box text-end d-none d-xl-block"></div>

          
          <% if (locals.user) { %>
            <!-- If user is logged in -->
            <div class="dropdown">
              <a class="sign-in-link"  >
                <%= locals.user.name %> <!-- Display user's name -->
              </a>
              <div class="dropdown-content" aria-labelledby="userDropdown">
                <a  href="/userProfile">Profile</a>
                <a href="/logout">Logout</a>
              </div>
            </div>
          <% } else { %>
            <!-- If user is not logged in -->
            <div class="auth-links">
              <a href="/signup" class="text-link mx-2" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Sign Up">
                Sign Up
              </a>
              <a href="/login" class="text-link mx-2" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Login">
                Login
              </a>
            </div>
          <% } %>
          
                  <!-- Wishlist Icon (Black Icon) -->

                  <a href="/wishlist" class="mx-2" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Wishlist">
                    <i class="fas fa-heart icon"></i>
                  </a>


                  <!-- Cart Icon (Black Icon) -->

                  <a href="/cart" class="mx-2" data-bs-toggle="tooltip" title="Cart">
                    <i class="fas fa-shopping-cart icon"></i>
                  </a>

        </div>
      </div>
    </div>


    <!-- Navigation Menu -->
    <div class="container-fluid">
      <div class="row py-3">
        <div>
          <nav class="main-menu d-flex navbar navbar-expand-lg">

            <button class="navbar-toggler" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasNavbar"
              aria-controls="offcanvasNavbar">
              <span class="navbar-toggler-icon"></span>
            </button>

            <div class="offcanvas offcanvas-end" tabindex="-1" id="offcanvasNavbar"
              aria-labelledby="offcanvasNavbarLabel">
              <div class="offcanvas-header justify-content-center">
                <button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="Close"></button>
              </div>
              <div class="offcanvas-body justify-content-center">
                <ul class="navbar-nav menu-list list-unstyled d-flex gap-md-3 mb-0">
                  <li class="nav-item active">
                    <a href="/" class="nav-link">Home</a>
                  </li>
                  <li class="nav-item dropdown">
                    <a href="/shop" class="nav-link">Shop All</a>
                  </li>
                  <li class="nav-item">
                    <a href="" class="nav-link">About Us</a>
                  </li>
                  <li class="nav-item">
                    <a href="#accessories" class="nav-link">Contact Us</a>
                  </li>
                  <li class="nav-item">
                    <a href="#brand" class="nav-link">Track Order</a>
                  </li>
                </ul>
              </div>
            </div>
          </nav>
        </div>
      </div>
    </div>
  </header> 
  <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js"></script>
